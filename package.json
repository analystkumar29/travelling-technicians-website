{
  "name": "travelling-technicians",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node start-dev-stable.js",
    "dev:legacy": "next dev",
    "dev:stable": "node start-dev-stable.js",
    "validate-env": "node scripts/validate-env.js",
    "prebuild": "npm run validate-env",
    "build": "SHARP_IGNORE_GLOBAL_LIBVIPS=1 next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "predeploy": "npm run build && touch out/.nojekyll",
    "deploy": "gh-pages -d out",
    "deploy:manual": "npm run build && touch out/.nojekyll && gh-pages -d out",
    "clean": "rm -rf .next && rm -rf out",
    "dev-simple": "node simple-start-dev.js",
    "postinstall": "npm run sharp-install",
    "sharp-install": "cd node_modules/sharp && npm install --force && cd ../..",
    "clean:auth": "node clean-auth-and-start.js",
    "extract:pricing": "npx tsx scripts/mobileactive/run-extraction.ts",
    "extract:pricing:fetch": "npx tsx scripts/mobileactive/run-extraction.ts fetch-only",
    "extract:pricing:transform": "npx tsx scripts/mobileactive/run-extraction.ts transform-only",
    "extract:pricing:test": "npx tsx scripts/mobileactive/test-setup.ts",
    "db:optimize": "node scripts/optimize-database.js",
    "db:performance-test": "node scripts/test-database-performance.js",
    "api:performance-test": "node scripts/test-api-performance.js",
    "system:health-check": "node scripts/system-health-check.js",
    "optimization:full": "npm run db:optimize && npm run api:performance-test && npm run system:health-check",
    "data:clean": "node scripts/ai-data-cleaner.js",
    "data:clean:enhanced": "node scripts/ai-data-cleaner-enhanced.js",
    "data:clean:advanced": "node scripts/mobileactive/run-advanced-cleaning.js",
    "data:clean:advanced:test": "node scripts/mobileactive/run-advanced-cleaning.js --test",
    "data:clean:advanced:full": "node scripts/mobileactive/run-advanced-cleaning.js --full",
    "db:rebuild": "node scripts/rebuild-database.js",
    "db:rebuild:enhanced": "node scripts/rebuild-database-enhanced.js",
    "db:full-rebuild": "node scripts/master-database-rebuild.js",
    "db:full-rebuild:enhanced": "node scripts/master-database-rebuild-enhanced.js",
    "db:full-rebuild:dry": "node scripts/master-database-rebuild.js --dry-run",
    "db:full-rebuild:verbose": "node scripts/master-database-rebuild.js --verbose",
    "db:enhanced-rebuild": "npm run data:clean:enhanced && npm run db:rebuild:enhanced"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@radix-ui/react-slot": "^1.2.3",
    "@sendgrid/mail": "^8.1.5",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.50.3",
    "@tanstack/react-query": "^5.76.2",
    "@tanstack/react-query-devtools": "^5.76.2",
    "autoprefixer": "^10.4.16",
    "axios": "^1.10.0",
    "chalk": "^4.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "framer-motion": "^10.16.4",
    "fuse.js": "^7.1.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.511.0",
    "next": "^14.2.25",
    "p-limit": "^5.0.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-confetti-explosion": "^2.1.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-icons": "^4.11.0",
    "react-leaflet": "^4.2.1",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.27.1",
    "@types/leaflet": "^1.9.17",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "ajv": "^8.17.1",
    "babel-loader": "^10.0.0",
    "canvas": "^3.1.0",
    "copy-webpack-plugin": "^13.0.0",
    "dotenv": "^16.6.1",
    "eslint": "^8.51.0",
    "eslint-config-next": "14.2.25",
    "gh-pages": "^6.3.0",
    "glob": "^11.0.2",
    "node-fetch": "^2.7.0",
    "sharp": "^0.34.2",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
