{
  "name": "travelling-technicians",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node start-dev-stable.js",
    "dev:legacy": "next dev",
    "dev:stable": "node start-dev-stable.js",
    "build": "SHARP_IGNORE_GLOBAL_LIBVIPS=1 next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "predeploy": "npm run build && touch out/.nojekyll",
    "deploy": "gh-pages -d out",
    "deploy:manual": "npm run build && touch out/.nojekyll && gh-pages -d out",
    "clean": "rm -rf .next && rm -rf out",
    "dev-simple": "node simple-start-dev.js",
    "postinstall": "npm run sharp-install",
    "sharp-install": "cd node_modules/sharp && npm install --force && cd ../.."
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@sendgrid/mail": "^8.1.5",
    "autoprefixer": "^10.4.16",
    "date-fns-tz": "^3.2.0",
    "framer-motion": "^10.16.4",
    "leaflet": "^1.9.4",
    "next": "^12.3.4",
    "postcss": "^8.4.31",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.47.0",
    "react-icons": "^4.11.0",
    "react-leaflet": "^4.2.1",
    "sharp": "^0.34.1",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@babel/runtime": "^7.27.1",
    "@supabase/supabase-js": "^2.49.4",
    "@types/leaflet": "^1.9.17",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "babel-loader": "^10.0.0",
    "canvas": "^3.1.0",
    "dotenv": "^16.5.0",
    "eslint": "^8.51.0",
    "eslint-config-next": "13.5.4",
    "gh-pages": "^6.3.0",
    "glob": "^11.0.2",
    "node-fetch": "^2.7.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}